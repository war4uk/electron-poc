  "scripts": {
    "deleteDist": "del dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/firstModule.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/firstModule.umd.js",
    "copyRelease": "cpx release.json dist && cd dist && del package.json && rename release.json package.json && cd ..",
    "copydts": "cpx *.d.ts dist",
    "copyhtml": "cpx *.html dist",
    "copycss": "cpx *.css dist",
    "build": "npm run deleteDist && npm run transpile && npm run package && npm run minify && npm run copyRelease && npm run copydts && npm run copyhtml && npm run copycss",
    "packager": "ng-packagr -p ng-package.json"
  },
